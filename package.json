{
  "name": "jquasi",
  "version": "1.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/LucaRainone/jquasi"
  },
  "description": "A very tiny jquery-syntax-like library for DOM manipulation and listeners. IE9+ compatible",
  "keywords": [
    "dom",
    "javascript",
    "browser"
  ],
  "watch": {
    "build": "src/*.js"
  },
  "scripts": {
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "build": "node cutdefine.js | \"node_modules/.bin/uglifyjs\" -o dist/jquasi.min.js --compress --mangle --comments=/@license/ -",
    "watch": "npm-watch"
  },
  "main": "dist/jquasi.min.js",
  "author": "Luca Rainone",
  "license": "MIT",
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jasmine": "^3.1.0",
    "jquery": "^3.3.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-requirejs": "^1.1.0",
    "npm-watch": "^0.5.0",
    "requirejs": "^2.3.5",
    "uglifyjs": "^2.4.11"
  }
}
